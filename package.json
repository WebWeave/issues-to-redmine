{
  "name": "github-to-redmine",
  "version": "1.0.0",
  "description": "Send github issues to redmine",
  "author": "Sander de Jonge <s.de.jonge@x-interactive.nl>",
  "license": "ISC",
  "repository": "https://github.com//github-to-redmine.git",
  "homepage": "https://github.com//github-to-redmine",
  "bugs": "https://github.com//github-to-redmine/issues",
  "keywords": [
    "probot",
    "github",
    "probot-app"
  ],
  "scripts": {
    "dev": "nodemon",
    "start": "probot run ./index.js",
    "lint": "standard --fix",
    "test": "jest && standard",
    "test:watch": "jest --watch --notify --notifyMode=change --coverage"
  },
  "dependencies": {
    "node-redmine": "^0.2.1",
    "probot": "^7.2.0"
  },
  "devDependencies": {
    "jest": "^24.0.0",
    "nock": "^10.0.0",
    "nodemon": "^1.17.2",
    "smee-client": "^1.0.2",
    "standard": "^12.0.1"
  },
  "engines": {
    "node": ">= 8.3.0"
  },
  "standard": {
    "env": [
      "jest"
    ]
  },
  "nodemonConfig": {
    "exec": "npm start",
    "watch": [
      ".env",
      "."
    ]
  },
  "jest": {
    "testEnvironment": "node"
  }
}
